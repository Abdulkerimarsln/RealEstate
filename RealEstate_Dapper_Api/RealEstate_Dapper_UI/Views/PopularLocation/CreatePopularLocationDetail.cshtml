
@{
    ViewData["Title"] = "CreatePopularLocationDetail";
    Layout = "~/Views/Admin/Index.cshtml";
}
@model RealEstate_Dapper_UI.Dtos.PopularLocationDtos.CreatePopularLocationDto
<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-12">
            <div class="bg-light rounded h-100 p-4">
                <h6 class="mb-4">Yeni Lokasyonlar Sayfası</h6>
                <form method="post">
                    <label>Şehir İsmi</label>
                    <input type="text" asp-for="CityName" class="form-control">
                    <br />                 

                    <label>Resim</label>
                    <input type="text" asp-for="ImageUrl" class="form-control">
                    <br />
                    <button class="btn btn-success">Lokasyonu Kaydet</button>
                </form>
            </div>
        </div>
    </div>
</div>


